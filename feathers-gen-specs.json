{
  "options": {
    "ver": "1.0.0",
    "inspectConflicts": false,
    "semicolons": false,
    "freeze": [
      "src/index.js"
    ],
    "ts": false
  },
  "app": {
    "environmentsAllowingSeedData": "",
    "seedData": false,
    "name": "aurora-api",
    "description": "API backend for The Aurora Project",
    "src": "src",
    "packager": "npm@>= 3.0.0",
    "providers": [
      "rest",
      "socketio"
    ]
  },
  "services": {
    "keys": {
      "name": "keys",
      "nameSingular": "key",
      "subFolder": "",
      "fileName": "keys",
      "adapter": "memory",
      "path": "/keys",
      "isAuthEntity": false,
      "graphql": false
    },
    "users": {
      "name": "users",
      "nameSingular": "user",
      "subFolder": "",
      "fileName": "users",
      "adapter": "mongoose",
      "path": "/users",
      "isAuthEntity": false,
      "graphql": true
    }
  },
  "hooks": {},
  "connections": {
    "mongoose": {
      "database": "mongodb",
      "adapter": "mongoose",
      "connectionString": "mongodb://localhost:27017/aurora_api"
    }
  }
}
